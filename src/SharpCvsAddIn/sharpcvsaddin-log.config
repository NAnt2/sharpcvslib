<log4net>
	<appender name="A1" type="log4net.Appender.DebugAppender" >
		<!-- <file value="logfile.txt" /> -->
		<layout type="log4net.Layout.PatternLayout">
			<conversionPattern value="%date [%thread] %-5level %logger [%ndc] - %message%newline" />
		</layout>
	</appender>
	<root>
		<level value="WARN" />
		<appender-ref ref="A1" />
	</root>
</log4net>

